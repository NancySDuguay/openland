      <div class="col-lg-12">
        <div class="panel panel-default">
          <div class="panel-heading">
            Registered users
          </div>
          <!-- /.panel-heading -->
            <div class="panel-body">
              <%= link_to 'Add user', new_user_path, class: "btn" %>
              <div class="table-responsive table-bordered">
                <table class="table">
                  <thead>
                    <tr>
                      <th>#</th>
                      <th>Username</th>
                      <th>First Name</th>
                      <th>Last Name</th>
                      <th>Email</th>
                      <th>Actions</th>
                    </tr>
                  </thead>
                  <tbody>
                  <% @users.each do |user| %>
                    <% if user != current_user %>
                    <tr>
                      <td></td>
                      <th><%= user.username %></th>
                      <td><%= user.first_name %></td>
                      <td><%= user.last_name %></td>
                      <td><%= user.email %></td>
                      <td>
                        <%= link_to 'Show', user_path(user) %> |
                        <%= link_to 'Edit', edit_user_path(user) %> |
                        <%= link_to 'Destroy', user_path(user),
                            method: :delete,
                            data: { confirm: 'Are you sure?' } %>
                      </td>
                    </tr>
                    <% end %>
                  <% end %>
                  </tbody>
                </table>
              </div>
              <!-- /.table-responsive -->
            </div>
          <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
      </div>
      <!-- /.col-lg-12 -->
